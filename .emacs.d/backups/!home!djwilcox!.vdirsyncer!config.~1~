[general]
status_path = ~/.vdirsyncer/status/

# Google - caldav
[pair calendar]
a = calendar_local
b = calendar_remote
metadata = ["color", "displayname"]
collections = ["from b", "from a"]
conflict_resolution = a wins

[storage calendar_local]
type = filesystem
path = ~/.calendars/google/
fileext = .ics

[storage calendar_remote]
type = google_calendar
token_file = ~/.vdirsyncer/google-token-calendars
client_id = "314206871896-qmgkjvqf8iqttjdt4p89kneapkc3f5vh.apps.googleusercontent.com"
client_secret = "8pWYhPVwDYkix-Ohb4m9ox-m"

